<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="RRHHWebServicePortBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/ARSACSoftWebServices/RRHHWebService"
                binding="basicHttpBinding" bindingConfiguration="RRHHWebServicePortBinding"
                contract="RRHHWebService.RRHHWebService" name="RRHHWebServicePort" />
        </client>
    </system.serviceModel>
</configuration>